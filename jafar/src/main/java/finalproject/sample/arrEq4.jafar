func vectorSum(int[3] x,y): int[3] {
    int[3] sum;
    int i;
    i:= 0;
    while (i < 3): {
        sum[i]:= sum[i]+x[i]*y[i];
        i:=i+1;
    }
    return sum;
}

{
    int [3] a,b,res;
    int [1][3] expected;
