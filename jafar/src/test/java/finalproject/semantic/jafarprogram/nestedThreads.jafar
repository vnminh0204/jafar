Program nestedThreads;

shared int a;
shared int b;
shared int c;
{
    a := 67;
    b := 89;
    parbegin:
    {
        parbegin:
        {
            lock();
            a := a + 1;
            b := b + 1;
            print(a);
            unlock();
        }
        {
            lock();
            a := a + 12;
            b := b + 16;
            print(b);
            unlock();

        }

        parend;
    }
    {
        c := 19;
        print(c);
    }
    parend;
}