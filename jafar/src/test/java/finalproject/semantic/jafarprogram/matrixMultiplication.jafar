Program vectorSum;

int[2][3] a;
int [3][2] b;
int [2][2] res;


{
    int i,j,k;
    res := [[0,0],[0,0]];
    a:= [[1,2,3], [4,5,6]];
    b:= [[10,11],[20,21],[30,31]];
    i:= 0;
    while (i < 2): {
